<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<SplitPane dividerPositions="0.2" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kodedu.terminalfx.FXMLController">
   <items>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <center>
            <TextArea fx:id="txtCommand" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <HBox spacing="5.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#btnSaveOnAction" text="Save" />
                  <Button mnemonicParsing="false" onAction="#btnExecOnAction" text="Exec" />
               </children>
            </HBox>
         </bottom>
      </BorderPane>
      <AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0">
        <children>
          <VBox prefHeight="500.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" styleClass="floating" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS" />
          </VBox>
        </children>
      </AnchorPane>
   </items>
</SplitPane>
